{
  "name": "elements-sk-local",
  "version": "2.2.0",
  "private": true,
  "description": "A set of light-weight custom elements with a uniform style.",
  "main": "index.js",
  "homepage": "https://github.com/google/skia-buildbot/tree/master/ap/elements-sk",
  "license": "Apache-2.0",
  "repository": "https://github.com/google/elements-sk.git",
  "scripts": {
    "clean": "rimraf ./elements-sk ./dist ./src/icon/*.ts",
    "build": "npm run build-icons && npm run build-typescript && npm run build-assets && npm run build-demo-pages",
    "build-icons": "go run generate_icons.go",
    "build-typescript": "npx tsc",
    "build-assets": "rsync -avz src/ elements-sk/ --exclude '*.js' --exclude '*.ts'",
    "build-demo-pages": "npx webpack --mode=production",
    "test": "npx karma start --single-run --browsers ChromeHeadless",
    "build:ci": "npm ci && npm run clean && npm run build && npm run test"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/jasmine": "^3.5.10",
    "@webcomponents/custom-elements": "^1.2.4",
    "autoprefixer": "^9.7.6",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "extract-text-webpack-plugin": "^v4.0.0-beta.0",
    "glob": "^7.1.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.5.0",
    "jsdoc": "^3.6.3",
    "karma": "^6.3.16",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^3.1.1",
    "karma-typescript": "^5.0.2",
    "material-design-icons": "^3.0.1",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.1",
    "sinon": "^7.3.2",
    "typescript": "^3.8.3",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2"
  }
}
