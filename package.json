{
  "name": "elements-sk-local",
  "version": "2.2.0",
  "private": true,
  "description": "A set of light-weight custom elements with a uniform style.",
  "main": "index.js",
  "homepage": "https://github.com/google/skia-buildbot/tree/master/ap/elements-sk",
  "license": "Apache-2.0",
  "repository": "https://github.com/google/elements-sk.git",
  "scripts": {
    "clean": "rimraf ./elements-sk ./dist",
    "build": "npm run build-typescript && npm run build-assets",
    "build-typescript": "tsc",
    "build-assets": "rsync -avz src/ elements-sk/ --exclude '*.js' --exclude '*.ts'",
    "test": "npx karma start --single-run --browsers=ChromeHeadlessCustom",
    "build:ci": "npm ci && npm run clean && npm run build && npm run test"
  },
  "devDependencies": {
    "@webcomponents/custom-elements": "^1.2.4",
    "autoprefixer": "^9.7.6",
    "chai": "^4.2.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "extract-text-webpack-plugin": "^v4.0.0-beta.0",
    "glob": "^7.1.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.6.3",
    "karma": "^3.1.3",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-mocha": "^1.3.0",
    "karma-sinon": "^1.0.5",
    "karma-webpack": "3.0.5",
    "material-design-icons": "^3.0.1",
    "mocha": "^5.2.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^3.0.2",
    "sinon": "^7.3.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2"
  }
}
